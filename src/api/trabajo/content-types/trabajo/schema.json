{
  "kind": "collectionType",
  "collectionName": "trabajos",
  "info": {
    "singularName": "trabajo",
    "pluralName": "trabajos",
    "displayName": "Trabajo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "descripcion": {
      "type": "blocks",
      "required": true
    },
    "nombreAutor": {
      "type": "string",
      "required": true
    },
    "apellidoAutor": {
      "type": "string",
      "required": true
    },
    "emailAutor": {
      "type": "email",
      "required": true
    },
    "cedulaAutor": {
      "type": "biginteger",
      "required": true,
      "unique": false
    },
    "PDF": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "downloader": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "aprobado": {
      "type": "boolean",
      "default": false
    },
    "categoria": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::categoria.categoria"
    }
  }
}
